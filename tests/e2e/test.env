# E2Eテスト環境変数設定
# Note: Stripe認証情報はGitHub Secretsで管理
# STRIPE_SECRET_KEY: GitHub Secretsで設定済み
# NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY: GitHub Secretsで設定済み

# プラットフォーム設定（テスト環境用）
PLATFORM_FEE_RATE=0.10
MINIMUM_PAYOUT_AMOUNT=1000

# テスト実行設定
PLAYWRIGHT_BASE_URL=http://localhost:8888
PLAYWRIGHT_TIMEOUT=45000
PLAYWRIGHT_RETRIES=2
PLAYWRIGHT_WORKERS=1

# MCP連携設定
MCP_ENABLED=true
MCP_AUTO_CLEANUP=true

# OAuth認証設定（テスト用モック）
TEST_OAUTH_MOCK_ENABLED=true
TEST_OAUTH_PROVIDER=google

# デバッグ設定
NEXT_PUBLIC_ENABLE_DEBUG_LOGGING=true
NEXT_PUBLIC_LOG_LEVEL=3

# Note: Supabase認証情報はGitHub Secretsで管理
# NEXT_PUBLIC_SUPABASE_URL: GitHub Secretsで設定済み
# NEXT_PUBLIC_SUPABASE_ANON_KEY: GitHub Secretsで設定済み