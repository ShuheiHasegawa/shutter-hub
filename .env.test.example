# MCP連携設定
MCP_ENABLED=true
MCP_TEST_ENVIRONMENT=true
MCP_AUTO_CLEANUP=true
MCP_TEST_DATA_SEED=true

# テスト用Supabase設定
# Supabaseダッシュボードから新しいPublishable Keyを取得してください
# Project Settings > API > Publishable keys
TEST_SUPABASE_URL=https://your-project-id.supabase.co
# 新形式のPublishable Key（推奨）
TEST_SUPABASE_PUBLISHABLE_KEY=sb_publishable_your_key_here
# または従来のAnon Key（後方互換性のため）
# TEST_SUPABASE_ANON_KEY=your_anon_key_here

# Service Role Key（サーバーサイドのみ、絶対に公開しないこと）
# Project Settings > API > Service role key
TEST_SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# テスト用認証情報
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=testpassword123